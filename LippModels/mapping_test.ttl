###########################################
# Add patient 
###########################################
map:patientQuery a rr:LogicalTable;
    rr:sqlQuery """SELECT 
    gen_idnumber as id 
        FROM gen_patient;""";
    rdfs:label "";
    skos:definition "".

map:patient a rr:TriplesMap;
    rr:logicalTable map:patientQuery;
    rr:subjectMap [
        rr:template "patient/{id}";
        rr:class ncit:C16960; #patient
    ].

###########################################
# Add identifier                                
###########################################
map:identifier a rr:TriplesMap;
    rr:logicalTable map:patientQuery;
    rr:subjectMap [
        rr:template "patient/{id}/";
        rr:class ncit:C25364; #identifier
    ];
    rr:predicateObjectMap [
        rr:predicate roo:P100042; #has_value
        rr:objectMap [
            rr:column "id"; 
            rr:datatype xsd:string;
        ];
    ].

map:patient rr:predicateObjectMap [
        rr:predicate roo:P100061; #has_person_identifier
        rr:objectMap [
            rr:parentTriplesMap map:identifier;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ].


###########################################
# Add toxicities
###########################################
map:headNeckToxQuery a rr:LogicalTable;
    rr:sqlQuery """SELECT 
    gen_idnumber as id, 
    hn_tox_id,
    hn_tox_xerostomia, 
    hn_tox_dysph,
    hn_tox_timepoint
        FROM hn_tox;""";
    rdfs:label "";
    skos:definition "".

map:toxID a rr:TriplesMap;
    rr:logicalTable map:headNeckToxQuery;
    rr:subjectMap [
        rr:template "patient/{id}/tox/{hn_tox_id}";
        rr:class roo:toxID;  #placeholder
    ];
    rr:predicateObjectMap [
        rr:predicate roo:has_value;
        rr:objectMap [
            rr:column "hn_tox_id"; 
            rr:datatype xsd:string;
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate roo:has_timepoint; #new roo predicate
        rr:objectMap [
            rr:parentTriplesMap map:timepoint;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];        
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate roo:has_xero; #new roo predicate
        rr:objectMap [
            rr:parentTriplesMap map:xero;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];        
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate roo:has_dysph; #new roo predicate
        rr:objectMap [
            rr:parentTriplesMap map:dysph;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ].

map:timepoint a rr:TriplesMap;
    rr:logicalTable map:headNeckToxQuery;
    rr:subjectMap [
        rr:template "patient/{id}/tox/{hn_tox_timepoint}";
        rr:class ncit:C25646; #registration
    ];
    rr:predicateObjectMap [
        rr:predicate roo:P100042; #has_value
        rr:objectMap [
            rr:column "hn_tox_timepoint"; 
            rr:datatype xsd:string;
        ];
    ].     

map:xero a rr:TriplesMap;
    rr:logicalTable map:headNeckToxQuery;
    rr:subjectMap [
        rr:template "patient/{id}/tox/{hn_tox_xerostomia}";
        rr:class roo:xero; #clinics and hospitals 
    ];
    rr:predicateObjectMap [
        rr:predicate roo:P100042; #has_value
        rr:objectMap [
            rr:column "hn_tox_xerostomia"; 
            rr:datatype xsd:string;
        ];
    ].

map:dysph rr:TriplesMap;
    rr:logicalTable map:headNeckToxQuery;
    rr:subjectMap [
        rr:template "patient/{id}/tox/{hn_tox_dysph}";
        rr:class roo:dysph; #clinics and hospitals 
    ];
    rr:predicateObjectMap [
        rr:predicate roo:P100042; #has_value
        rr:objectMap [
            rr:column "hn_tox_dysph"; 
            rr:datatype xsd:string;
        ];
    ].

map:patient rr:predicateObjectMap [
        rr:predicate roo:has_toxID; #is_treated_in
        rr:objectMap [
            rr:parentTriplesMap map:toxID;
            rr:joinCondition [
                rr:child "id";
                rr:parent "id";
            ];
        ];
    ].



